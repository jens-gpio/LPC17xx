#!/bin/bash

if [ -x ./generate ]; then generate=./generate; fi
if [ -x ../bin/generate ]; then generate=../bin/generate; fi
if [ -x ./bin/generate ]; then generate=./bin/generate; fi

if [ -x $generate ]; then
	$generate lpc175x_6x lpc177x_8x
else
	echo "### Error: 'generate' script not found. Please run 'g' from within the repository."
fi
